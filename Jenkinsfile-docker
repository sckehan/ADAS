
pipeline {
    agent any
    stages {
        stage('Pull') {
            steps {
                git 'https://github.com/sckehan/ADAS.git'
            }
        }
        
        stage('Deploy'){
          sh "kubectl version"
          sh "kubectl create -f tomcat.yaml"
        }
    }
}


